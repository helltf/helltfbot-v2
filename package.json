  "scripts": {
    "test": "set NODE_ENV=test&&jasmine",
    "unit-tests": "set NODE_ENV=test&&jasmine --config=spec/support/jasmine-unit.json",
    "integration-tests": "set NODE_ENV=test&&jasmine --config=spec/support/jasmine-integration.json",
    "build": "tsc",
    "deploy": "sh deploy.sh",
    "prettier-format": "prettier --config .prettierrc ./**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helltf/helltfbot-v2.git"
