{
  "name": "helltfbot-v2",
  "version": "1.3.0",
  "description": "version 2 for helltfbot",
  "main": "",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:no-output": "jest --noStackTrace",
    "prettier-format": "prettier --config .prettierrc ./**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "migration": "npx typeorm-ts-node-commonjs migration:run -d migration-datasource.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d migration-datasource.ts ./src/db/migration/migration",
    "serve": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only index.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helltf/helltfbot-v2.git"
  },
  "author": "helltf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helltf/helltfbot-v2/issues"
  },
  "homepage": "https://github.com/helltf/helltfbot-v2#readme",
  "dependencies": {
    "@types/node-fetch": "^2.6.1",
    "@types/tmi.js": "^1.8.1",
    "chalk": "^4.1.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "jaro-winkler": "^0.2.8",
    "jest": "^28.1.3",
    "npm": "^8.6.0",
    "pg": "^8.7.3",
    "reconnecting-websocket": "^4.4.0",
    "redis": "^4.1.0",
    "strict-event-emitter-types": "^2.0.0",
    "tmi.js": "^1.8.5",
    "ts-jest": "^28.0.8",
    "typeorm": "^0.3.5",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/jaro-winkler": "^0.2.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^17.0.23",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "humanize-duration-ts": "^2.1.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.7.2"
  }
}
