    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.1.0",
    "prettier": "^2.6.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  }
